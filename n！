//使用循环的方式
#include<stdio.h>
int main(void) {
	int n = 0;
	int m=1;
	scanf_s("%d", &n);
	for (int i = 1; i <= n; i++) {
			m = i * m;
	}
	printf("%d", m);
	return 0;
}



//使用递归的方式
#include<stdio.h>
Fac(int x) {
	if (x <= 1)
		return 1;
	else
		return x*Fac(x - 1);
}
int main() {
	int n = 0;
	int ret = 0;
	scanf_s("%d", &n);
	ret = Fac(n);
	printf("%d\n", ret);
		return 0;
}
